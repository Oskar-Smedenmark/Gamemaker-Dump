/// get_index_with_element(curElement)
var curElement = argument0;
switch(curElement)
{
    case Element.white:
        return 0;
    break;
    case Element.red:
        return 1;
    break;
    case Element.green:
        return 2;
    break;
    case Element.blue:
        return 3;
    break;
    case Element.purple:
        return 4;
    break;
    case Element.yellow:
        return 5;
    break;
    case Element.cyan:
        return 6;
    break;
}
return Element.white;
